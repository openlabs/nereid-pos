<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        
        <record id="pos_url_map" model="nereid.url_map">
            <field name="name">POS</field>
        </record>
            
        <record id="products" model="nereid.url_rule">
            <field name="rule">/&lt;language&gt;/pos/products</field>
            <field name="endpoint">product.product.pos_list</field>
            <field name="sequence" eval="140" />
            <field name="http_method_get" eval="True"/>
            <field name="url_map" ref="pos_url_map" />
        </record>

        <record id="categories" model="nereid.url_rule">
            <field name="rule">/&lt;language&gt;/pos/categories</field>
            <field name="endpoint">product.category.pos_list</field>
            <field name="sequence" eval="140" />
            <field name="http_method_get" eval="True"/>
            <field name="url_map" ref="pos_url_map" />
        </record>
                
    </data>
</tryton>
